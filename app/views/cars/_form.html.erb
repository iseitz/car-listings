<%=form_for @car do |f| %>

<%=f.label :manufacturer %>
<%=f.collection_select :manufacturer_id, @manufacturers, :id, :name, include_blank: true %>

<%=f.label :name %>
<%=f.text_field :name %>

<%=f.label :color%>
<%=f.text_field :color %>

<%=f.label :year %>
<%=f.text_field :year %>

<%=f.label :mileage %>
<%=f.text_field :mileage %>

<%=f.label :description%>
<%=f.text_area:description %>

<%=f.submit 'Create Car', class: 'btn-primary' %>
<% end %>
