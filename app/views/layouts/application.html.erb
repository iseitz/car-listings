<!DOCTYPE html>
<html>
  <head>
    <title>CarListings</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
  </head>

  <body>
    <div class="notifications">
          <span class="notice"><%= notice %></span>
          <span class="alert"><%= alert %></span>
    </div>
    <%= yield %>
  </body>
</html>
